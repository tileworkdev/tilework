{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "LoadBalancing": {
    "Backend" : "haproxy",
    "BackendImage" : "tilework/loadbalancer-haproxy:latest"
  },
  "CertificateManagement": {
    "AcmeVerificationImage" : "tilework/acmevalidator:latest"
  },
  "Monitoring": {
    "Backend" : "collectd",
    "BackendImage" : "tilework/monitoring-collectd:latest"
  },
  "ConnectionStrings": {
    "DefaultConnection" : "Data Source=/var/lib/tilework/data.db"
  },
  "AllowedHosts": "*"
}
