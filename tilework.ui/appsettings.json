{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "LoadBalancing": {
    "Backend" : "haproxy",
    "BackendImage" : "tilework/loadbalancer-haproxy:latest"
  },
  "CertificateManagement": {
    "AcmeVerificationImage" : "tilework/acmevalidator:latest",
    "CertRenewalLeadTime" : "7.00:00:00"
  },
  "Monitoring": {
    "DataCollector" : {
      "Backend" : "telegraf",
      "BackendImage" : "telegraf:1.36"
    },
    "DataPersistence" : {
      "Backend" : "influxdb",
      "BackendImage" : "tilework/monitoring-influxdb:latest"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection" : "Data Source=/var/lib/tilework/data.db"
  },
  "AllowedHosts": "*"
}
